num=int(input("valor>>>"))
x=1
if num<2:
    print('1s1-')
    print('cv=1')
elif num==2:
    print('1s2-')
    print('cv=2')
elif num<4:
    print('1s2-2s{}-'.format(-(num-4)))
    print('cv={}'.format(-(num-4)))
elif num==4:
    print('1s2-2s2-')
    print('cv=2')
elif num<10:
    print('1s2-2s2-2p{}-'.format((num-4)))
    print('cv={}'.format((num-4)+2))
elif num==10:
    print('1s2-2s2-2p6-')
elif num<12:
    print('1s2-2s2-2p6-3s-{}'.format(num-10))
elif num==12:
    print('1s2-2s2-2p6-3s2-')
elif num<18:
    print('1s2-2s2-2p6-3s2-3p{}-'.format(num-12))
elif num==18:
    print('1s2-2s2-2p6-3s2-3p6')
elif num<20:
    print('1s2-2s2-2p6-3s2-3p6-4s{}'.format(num-18))
elif num==20:
    print('1s2-2s2-2p6-3s2-3p6-4s2')
elif num<30:
    print('1s2-2s2-2p6-3s2-3p6-4s2-3d{}'.format(num-20))
elif num==30:
    print('1s2-2s2-2p6-3s2-3p6-4s2-3d10')
elif num<36:
    print('1s2-2s2-2p6-3s2-3p6-4s2-3d10-4p{}'.format(num-30))
elif num==36:
